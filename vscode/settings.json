{
  "breadcrumbs.enabled": false,
  "editor.codeLens": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorWidth": 3,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "JetBrains Mono, Monaco",
  "editor.fontLigatures": "'ss02', 'calt'",
  "editor.fontSize": 13,
  "editor.fontWeight": "normal",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.formatOnType": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlineSuggest.enabled": true,
  "editor.links": false,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "editor.rulers": [80],
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggestFontSize": 12,
  "editor.suggestLineHeight": 20,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.unfoldOnClickAfterEndOfLine": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 1680,
  "eslint.validate": ["javascript", "typescript", "svelte"],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": true,
  "explorer.decorations.colors": true,
  "explorer.incrementalNaming": "smart",
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "files.enableTrash": false,
  "files.trimTrailingWhitespace": true,
  "git.decorations.enabled": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "interactiveWindow.alwaysScrollOnNewCell": true,
  "javascript.suggest.autoImports": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindow.cellMarker.decorateCells": "None",
  "jupyter.interactiveWindow.cellMarker.default": "# %%",
  "jupyter.interactiveWindow.codeLens.enable": false,
  "jupyter.interactiveWindow.codeLens.enableGotoCell": false,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "markdown-preview-enhanced.enablePreviewZenMode": true,
  "plantuml.render": "PlantUMLServer",
  "plantuml.server": "http://localhost:8080",
  "prettier.configPath": "~/.prettierrc",
  "python.defaultInterpreterPath": "~/Development/playground/python/.venv/bin/python",
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "tailwindCSS.colorDecorators": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.letterSpacing": 1,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": false,
  "window.zoomLevel": -0.7,
  "workbench.colorCustomizations": {
    "[Atom One Light]": {
      "gitDecoration.addedResourceForeground": "#2db448",
      "gitDecoration.deletedResourceForeground": "#f42a2a",
      "gitDecoration.ignoredResourceForeground": "#8e8e90",
      "gitDecoration.modifiedResourceForeground": "#d5880b",
      "gitDecoration.untrackedResourceForeground": "#2db448"
    },
    "focusBorder": "#00000000",
    "terminal.foreground": "#839496",
    "terminal.background": "#002833",
    "terminal.ansiBlack": "#003541",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiGreen": "#859901",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiYellow": "#b58901",
    "terminal.ansiBrightBlack": "#586e75",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightMagenta": "#6c6ec6",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.ansiBrightYellow": "#657b83",
    "terminalCursor.foreground": "#839496",
    "terminalCursor.background": "#003541"
  },
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Atom One Light",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.openPositioning": "last",
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.fontAliasing": "default",
  "workbench.iconTheme": "file-icons-colourless",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.indent": 14,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.rulers": [120],
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.rulers": [88],
    "editor.tabSize": 4,
    "editor.wordWrapColumn": 88,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  }
}
